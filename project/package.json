{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173",
    "dev-backend": "nodemon --watch ./server.js --watch ./data server.js",
    "dev-all": "concurrently -n \"FRONTEND,BACKEND\" -c \"blue.bold,green.bold\" \"npm run dev\" \"npm run dev-backend\"",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "start": "node server.js",
    "start-production": "npm run build && npm run start",
    "kill-port": "kill-port 3001 5173 5174 5175 5176 5177 5178"
  },
  "dependencies": {
    "@emailjs/browser": "^4.3.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "framer-motion": "^12.19.1",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-intersection-observer": "^9.16.0",
    "react-router-dom": "^7.6.2",
    "react-to-print": "^3.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.35",
    "prettier": "^3.6.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.4.2"
  },
  "main": "server.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "A full-stack React + Express application with TypeScript",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
